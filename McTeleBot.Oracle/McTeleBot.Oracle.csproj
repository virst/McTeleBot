		<PackageReference Include="Microsoft.Extensions.Hosting" Version="6.0.0" />
	</ItemGroup>

	<ItemGroup>
	  <Using Include="Microsoft.Extensions.Hosting" />
	  <Using Include="Serilog" />
	</ItemGroup>

</Project>
